<script>
    function updateTipoLamina() {
        // Obtém o valor selecionado no primeiro select
        var laminaUmSelect = document.getElementById("lamina_um");
        var selectedValue = laminaUmSelect.options[laminaUmSelect.selectedIndex].value;

        // Atualiza o valor no segundo select
        var laminaTipoUmSelect = document.getElementById("lamina_tipo_um");
        laminaTipoUmSelect.value = selectedValue;

        // Verifica se a opção selecionada é "Colchonete" e define os valores nos campos apropriados
        if (selectedValue === "Colchão infantil") {
            document.getElementById("lamina_min_um").value = "7";
            document.getElementById("lamina_max_um").value = "---";
        } if (selectedValue === "Colchonete") {
            document.getElementById("lamina_min_um").value = "4";
            document.getElementById("lamina_max_um").value = "8";
        }
        if (selectedValue === "Colchão geral") {
            document.getElementById("lamina_min_um").value = "12";
            document.getElementById("lamina_max_um").value = "---";
        }
        if (selectedValue === "Colchão auxiliar") {
            document.getElementById("lamina_min_um").value = "5";
            document.getElementById("lamina_max_um").value = "---";
        }
        if (selectedValue === "Colchão box conjugado") {
            document.getElementById("lamina_min_um").value = "5";
            document.getElementById("lamina_max_um").value = "---";
        }
        if (selectedValue === "Colchão misto") {
            document.getElementById("lamina_min_um").value = "5";
            document.getElementById("lamina_max_um").value = "---";
        }
        if (selectedValue === "Composto") {
            document.getElementById("lamina_min_um").value = "3";
            document.getElementById("lamina_max_um").value = "---";
        } if (selectedValue === "---") {
            document.getElementById("lamina_min_um").value = "";
            document.getElementById("lamina_max_um").value = "";
        }

    }
</script>

