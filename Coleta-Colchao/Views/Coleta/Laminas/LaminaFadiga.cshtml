@model ColetaModel.LaminaFadigaRotativa
@{
    Layout = null;
}
@{
    ViewData["Title"] = "Ensaio Lamina Fadiga Dinâmica";
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/coleta.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <title>FADIGA DINÂMICA</title>
    <style>
        /*    .controlador-conforme {                                                                                                                                                                                                                    } */
        input {
            text-align: center;
        }
    </style>
</head>
<body>
    <form asp-controller="Coleta" asp-action="SalvarLaminaFadiga" asp-route-os="@ViewBag.os" asp-route-orcamento="@ViewBag.orcamento" asp-route-rev="@ViewBag.rev">
        <table class="tables table-bordered border-secondary  table-sm">
            <thead>
                <tr>
                    <td><img src="https://www.labsystem-nuvem.com.br/imagens_arq/imagens/img/LabSystem.png" class="imagem" /></td>
                    <td colspan="3" class="fundo-tabela">Coleta De Dados Lamina Fadiga Dinâmica</td>
                    <td class="fundo-tabela">N Os:</td>
                </tr>
                <tr>
                    <td colspan="4">Ensaios de Colchão de Espuma (Portaria Inmetro 35)</td>
                    <td rowspan="2"> @ViewBag.os</td>
                </tr>
                @if (Model == null)
                {
                    <tr>
                        <td>Inicio:</td>
                        <td><input class="form-control" type="date" asp-for="data_ini" /></td>
                        <td>Termino:</td>
                        <td><input class="form-control" type="date" asp-for="data_term" /></td>
                    </tr>
                }
                else
                {
                    <tr>
                        <td>Inicio:</td>
                        <td><input class="form-control" type="text" asp-for="data_ini" /></td>
                        <td>Termino:</td>
                        <td><input class="form-control" type="text" asp-for="data_term" /></td>
                    </tr>
                }

            </thead>
        </table>
        <br />
        <table class="tables table-bordered border-secondary  table-sm">
            <tr class="fundo-tabela">
                <td>Item</td>
                <td>Descrição</td>
                <td>Resultado</td>
            </tr>
            <tr>
                <td rowspan="30"></td>
                <td>
                    <b>Determinação da Fadiga Dinâmica</b>
                </td>
                @if (Model == null)
                {
                    <td rowspan="40"></td>
                }
                else
                {
                    <td rowspan="40">@Model.conforme</td>
                }
            </tr>

            <tr>
                <td>
                    Separar 3 corpos de prova e realizar as medições das seguintes dimesões: (Largura 380 ±10mm x comprimento 380±10mm x altura 50±2mm).
                </td>
            </tr>
            <tr>
                <td>
                    <table class="tables table-bordered border-secondary  table-sm">

                        <tr class="fundo-tabela">
                            <td colspan="10">
                                - Temperatura da execução do ensaio  (23 ± 2) °C, sem necessidade de controle de umidade.
                            </td>
                        </tr>
                        <tr class="fundo-tabela">
                            <td colspan="2">Data do acondicionamento</td>
                            <td colspan="2">Horario do acondicionamento</td>
                            <td colspan="2">Temperatura encontrada</td>
                            <td colspan="2">IM</td>
                        </tr>
                        <tr>
                            <td>Inicio:</td>
                            @if (Model == null)
                            {
                                <td><input asp-for="acond_inicio" class="form-control" type="date" /></td>
                            }
                            else
                            {
                                <td><input asp-for="acond_inicio" class="form-control" type="text" /></td>
                            }
                            <td>Inicio:</td>
                            <td><input asp-for="hora_inicio" class="form-control" type="time" /></td>
                            <td>Min:</td>
                            <td><input asp-for="temp_inicio" class="form-control" /></td>

                            <td rowspan="2"><input asp-for="im" class="form-control" style="height:80px; " /></td>
                        </tr>
                        <tr>

                            <td>Final:</td>
                            @if (Model == null)
                            {
                                <td><input asp-for="acond_final" class="form-control" type="date" /></td>
                            }
                            else
                            {
                                <td><input asp-for="acond_final" class="form-control" type="text" /></td>
                            }
                            <td>Final:</td>
                            <td><input asp-for="hora_final" class="form-control" type="time" /></td>
                            <td>Max:</td>
                            <td><input asp-for="temp_final" class="form-control" /></td>

                        </tr>
                        <tr>
                            <td colspan="4">Responsavel</td>
                            <td colspan="5"><input asp-for="responsavel_cond" class="form-control" /></td>
                        </tr>
                    </table>
                    <br />
                    <table class="tables table-bordered border-secondary  table-sm">
                        <tr>
                            <td class="fundo-tabela">Densidade (kg/m³)</td>
                            <td><input type="text" class="form-control" asp-for="densidade" id="densidade" /></td>
                            <td class="fundo-tabela">Tipo Espuma</td>
                            <td colspan="3">
                                <select asp-for="tipo_espuma" class="form-control" id="tipo_espuma">
                                    <option value="Convencional">Convencional</option>
                                    <option value="Alta resiliência">Alta resiliência</option>
                                    <option value="Macia">Macia</option>
                                    <option value="Hipermacia">Hipermacia</option>
                                    <option value="Visco elástica">Visco elástica</option>
                                    <option value="Aglomerado">Aglomerado</option>
                                    <option value="Látex">Látex</option>
                                    <option value="Rabatan">Rabatan</option>
                                </select>
                            </td>
                        </tr>
                        <tr class="fundo-tabela">
                            <td rowspan="10">---</td>
                            <td> Amostra 1º</td>
                            <td> Amostra 2º</td>
                            <td> Amostra 3º</td>
                        </tr>
                        <tr class="fundo-tabela">
                            <td>Espessura Inicial (mm)</td>
                            <td>Espessura Inicial (mm)</td>
                            <td>Espessura Inicial (mm)</td>
                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_um" value="@ViewBag.esp_ini_amostra_um_um" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_um" value="@ViewBag.esp_ini_amostra_dois_um" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_um" value="@ViewBag.esp_ini_amostra_tres_um" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_um" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_um" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_um" /></td>
                            }

                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_dois" value="@ViewBag.esp_ini_amostra_um_dois" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_dois" value="@ViewBag.esp_ini_amostra_dois_dois" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_dois" value="@ViewBag.esp_ini_amostra_tres_dois" /></td>
                            }
                            else
