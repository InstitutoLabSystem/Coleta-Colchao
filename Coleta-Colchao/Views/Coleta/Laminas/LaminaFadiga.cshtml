                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_oito" /></td>
                        </tr>
                        <tr>
                            <td>Media:</td>
                            <td><input type="text" class="form-control" readonly asp-for="media_esp_fin_um" /></td>
                            <td><input type="text" class="form-control" readonly asp-for="media_esp_fin_dois" /></td>
                            <td><input type="text" class="form-control" readonly asp-for="media_esp_fin_tres" /></td>
                        </tr>
                        <tr>
                            <td>Tempo repouso (min):</td>
                            <td><input type="text" class="form-control" asp-for="tempo_rep_esp_um" /></td>
                            <td><input type="text" class="form-control" asp-for="tempo_rep_esp_dois" /></td>
                            <td><input type="text" class="form-control" asp-for="tempo_rep_esp_tres" /></td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                @if (Model == null)
                {
                    <td>
                        Amostra 1:
                        P.E: <input type="text" class="controlador" style="width:100px;" asp-for="pe_um_um" readonly value="@ViewBag.media_espessura_um" /> - <input type="text" class="controlador" style="width:100px;" asp-for="pe_um_dois" readonly /> ÷ <input type="text" class="controlador" style="width:100px;" asp-for="pe_um_tres" value="@ViewBag.media_espessura_um" readonly /> x <input type="text" class="controlador" style="width:100px;" value="100" disabled />  = <input type="text" class="controlador" style="width:100px;" asp-for="pe_media_um" readonly />
                    </td>
                }
                else
                {
                    <td>
                        Amostra 1:
                        P.E: <input type="text" class="controlador" style="width:100px;" asp-for="pe_um_um" readonly /> - <input type="text" class="controlador" style="width:100px;" asp-for="pe_um_dois" readonly /> ÷ <input type="text" class="controlador" style="width:100px;" asp-for="pe_um_tres" /> x <input type="text" class="controlador" style="width:100px;" value="100" disabled />  = <input type="text" class="controlador" style="width:100px;" asp-for="pe_media_um" readonly />
                    </td>
                }

            </tr>
            <tr>
                @if (Model == null)
                {
