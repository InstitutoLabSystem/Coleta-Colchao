                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_dois" /></td>
                            }

                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_tres" value="@ViewBag.esp_ini_amostra_um_tres" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_tres" value="@ViewBag.esp_ini_amostra_dois_tres" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_tres" value="@ViewBag.esp_ini_amostra_tres_tres" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_tres" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_tres" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_tres" /></td>
                            }

                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_quatro" value="@ViewBag.esp_ini_amostra_um_quatro" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_quatro" value="@ViewBag.esp_ini_amostra_dois_quatro" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_quatro" value="@ViewBag.esp_ini_amostra_tres_quatro" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_quatro" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_quatro" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_quatro" /></td>
                            }

                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_cinco" value="@ViewBag.esp_ini_amostra_um_cinco" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_cinco" value="@ViewBag.esp_ini_amostra_dois_cinco" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_cinco" value="@ViewBag.esp_ini_amostra_tres_cinco" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_cinco" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_cinco" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_cinco" /></td>
                            }

                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_seis" value="@ViewBag.esp_ini_amostra_um_seis" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_seis" value="@ViewBag.esp_ini_amostra_dois_seis" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_seis" value="@ViewBag.esp_ini_amostra_tres_seis" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_seis" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_seis" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_seis" /></td>
                            }

                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_sete" value="@ViewBag.esp_ini_amostra_um_sete" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_sete" value="@ViewBag.esp_ini_amostra_dois_sete" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_sete" value="@ViewBag.esp_ini_amostra_tres_sete" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_sete" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_sete" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_sete" /></td>
                            }

                        </tr>
                        <tr>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_oito" value="@ViewBag.esp_ini_amostra_um_oito" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_oito" value="@ViewBag.esp_ini_amostra_dois_oito" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_oito" value="@ViewBag.esp_ini_amostra_tres_oito" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_um_oito" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_dois_oito" /></td>
                                <td><input type="text" class="form-control" asp-for="esp_ini_amostra_tres_oito" /></td>
                            }

                        </tr>

                        <tr>
                            <td>Media:</td>
                            @if (Model == null)
                            {
                                <td><input type="text" class="form-control" readonly asp-for="esp_ini_media_um" value="@ViewBag.esp_media_um" /></td>
                                <td><input type="text" class="form-control" readonly asp-for="esp_ini_media_dois" value="@ViewBag.esp_media_dois" /></td>
                                <td><input type="text" class="form-control" readonly asp-for="esp_ini_media_tres" value="@ViewBag.esp_media_tres" /></td>
                            }
                            else
                            {
                                <td><input type="text" class="form-control" readonly asp-for="esp_ini_media_um" /></td>
                                <td><input type="text" class="form-control" readonly asp-for="esp_ini_media_dois" /></td>
                                <td><input type="text" class="form-control" readonly asp-for="esp_ini_media_tres" /></td>
                            }

                        </tr>
                    </table>
                    <table class="tables table-bordered border-secondary  table-sm">
                        <tr>
                            <td class="fundo-tabela">
                                Média da espessura - 1° Amostra (mm)
                            </td>
                            <td><input type="text" class="form-control" readonly asp-for="media_espessura_um" /></td>
                        </tr>
                        <tr>
                            <td class="fundo-tabela">
                                Média da espessura - 2° Amostra (mm)
                            </td>
                            <td><input type="text" class="form-control" readonly asp-for="media_espessura_dois" /></td>
                        </tr>
                        <tr>
                            <td class="fundo-tabela">
                                Média da espessura - 3° Amostra (mm)
                            </td>
                            <td><input type="text" class="form-control" readonly asp-for="media_espessura_tres" /></td>
                        </tr>
                        <tr>
                            <td class="fundo-tabela">Média da espessura</td>
                            <td><input type="text" class="form-control" readonly asp-for="media_espessura_total" /></td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr>
                <td>
                    <b>Após realizado as medições dos corpos de prova, verificar qual é o tipo de espuma.</b>
                </td>
            </tr>
            <tr>

                <td>
                    <div class="form-check">

                        <input class="form-check-input" type="checkbox" id="checkbox">
                        <label class="form-check-label" for="checkbox">
                            <b>Espuma aglomerado</b> - Fator De compressão (50 ± 5) %
                        </label>
                    </div>
                    <br />
                    <div class="form-check">

                        <input class="form-check-input" type="checkbox" id="checkbox2">
                        <label class="form-check-label" for="checkbox2">
                            <b>Espuma Flexivel</b> - Fator De compressão (70 ± 5) %
                        </label>
                    </div>

                </td>
            </tr>
            <tr>
                <td>Amosta 1: Quantidade De Ciclos aplicados:  <input type="text" class="controlador" style="width:100px;" asp-for="ciclos_um" value="80000" readonly /></td>
            </tr>
            <tr>
                <td>Amosta 2: Quantidade De Ciclos aplicados:  <input type="text" class="controlador" style="width:100px;" asp-for="ciclos_dois" value="80000" readonly /></td>
            </tr>
            <tr>
                <td>Amosta 3: Quantidade De Ciclos aplicados:  <input type="text" class="controlador" style="width:100px;" asp-for="ciclos_tres" value="80000" readonly /></td>

            </tr>
            <tr>
                <td>
                    Após o término do ensaio, Deve-se retirar o corpo de prova do aparelho, e deixá-lo em repouso a (23 ± 2) °C por no mínimo 10 min e no máximo 30 min.
                </td>
            </tr>
            <tr>
                <td>
                    <table class="tables table-bordered border-secondary  table-sm">
                        <tr class="fundo-tabela">
                            <td rowspan="10">---</td>
                            <td> Amostra 1º</td>
                            <td> Amostra 2º</td>
                            <td> Amostra 3º</td>
                        </tr>
                        <tr class="fundo-tabela">
                            <td>Espessura Final (mm)</td>
                            <td>Espessura Final (mm)</td>
                            <td>Espessura Final (mm)</td>
                        </tr>
                        <tr>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_um_um" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_dois_um" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_um" /></td>
                        </tr>
                        <tr>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_um_dois" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_dois_dois" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_dois" /></td>
                        </tr>
                        <tr>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_um_tres" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_dois_tres" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_tres" /></td>
                        </tr>
                        <tr>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_um_quatro" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_dois_quatro" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_quatro" /></td>
                        </tr>
                        <tr>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_um_cinco" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_dois_cinco" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_cinco" /></td>
                        </tr>
                        <tr>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_um_seis" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_dois_seis" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_seis" /></td>
                        </tr>
                        <tr>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_um_sete" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_dois_sete" /></td>
                            <td><input type="text" class="form-control" asp-for="esp_final_amostra_tres_sete" /></td>
