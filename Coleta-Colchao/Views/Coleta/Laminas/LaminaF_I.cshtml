    <script>
        const tipo_espuma = document.getElementById("tipo_espuma");
        const densidade = document.getElementById("densidade");
        var reducao = document.getElementById("reducao");
        var reducao2 = document.getElementById("reducao2");
        var reducao3 = document.getElementById("reducao3");
        var condicao = document.getElementById("condicao");
        var condicao = document.getElementById("condicao");
        var tamanho = document.getElementById("tamanho");
        var especificado = document.getElementById("especificado");
        var conforto = document.getElementById("conforto");

        tipo_espuma.addEventListener('change', function () {
            mostrarResultados(tipo_espuma.value, densidade.value);
        });

        densidade.addEventListener('input', function () {
            console.log(densidade.value);
            mostrarResultados(tipo_espuma.value, densidade.value);
        });

        function mostrarResultados(opcaoSelecionada, valorDigitado) {
            let resultado = '';
            if (opcaoSelecionada === 'Convencional') {

                if (valorDigitado == '20') {
                    especificado.value = 95;
                    conforto.value = 2;
                    tamanho.value = 'Min';
                    console.log(especificado.value);
                } else if (valorDigitado == '18') {
                    especificado.value = 80;
                    conforto.value = 2;
                    tamanho.value = 'Min';
                } else if (valorDigitado == '23') {
                    especificado.value = 110;
                    conforto.value = "2,1";
                    tamanho.value = 'Min';
                } else if (valorDigitado == '26') {
                    especificado.value = 130;
                    conforto.value = "2,1";
                    tamanho.value = 'Min';
                } else if (valorDigitado == '28') {
                    especificado.value = 145;
                    conforto.value = "2,2";
                    tamanho.value = 'Min';
                }
                else if (valorDigitado == '33') {
                    especificado.value = 165;
                    conforto.value = "2,2";
                    tamanho.value = 'Min';
                }
                else if (valorDigitado == '40') {
                    especificado.value = 185;
                    conforto.value = "2,2";
                    tamanho.value = 'Min';
                }
                else if (valorDigitado == '45') {
                    especificado.value = 200;
                    conforto.value = "2,2";
                    tamanho.value = 'Min';
                }
                else {
                    especificado.value = "Não há especificado na tabela";
                }
                console.log(valorDigitado);
                console.log(conforto.value);

            } else if (opcaoSelecionada === 'Hipermacia') {
                if (valorDigitado == '20' || valorDigitado == '24') {
                    especificado.value = 35;
                    tamanho.value = 'Max';

                }
                else if (valorDigitado == '29') {
                    especificado.value = 50;
                }
                else if (valorDigitado >= '35') {
                    especificado.value = 150;
                    tamanho.value = 'Max';
                }
                else {
                    especificado.value = "Não há especificado na tabela";
                }
                conforto.value = "Não há especificado na tabela";
            } else if (opcaoSelecionada === 'Macia') {
                if (valorDigitado == '20') {
                    especificado.value = 95;
                    tamanho.value = 'Max';
                } else if (valorDigitado == '24') {
                    especificado.value = 130;
                    tamanho.value = 'Max';
                } else if (valorDigitado == '20') {
                    especificado.value = 165;
                    tamanho.value = 'Max';
                } else if (valorDigitado >= '35') {
                    especificado.value = 200;
                    tamanho.value = 'Max';
                }
                else {
                    especificado.value = "Não há especificado na tabela";
                }
                conforto.value = "Não há especificado na tabela";
            } else if (opcaoSelecionada === 'Aglomerado') {
                if (valorDigitado >= '65') {
                    especificado.value = 250;
                    tamanho.value = 'Max';
                }
                else {
                    reducao.value = 50;
                    especificado.value = "Não há especificado na tabela";
                }
                conforto.value = "Não há especificado na tabela";
            }
            else if (opcaoSelecionada === 'Visco elástica') {
                if (valorDigitado >= '30') {
                    especificado.value = 25;
                    conforto.value = "1,8";
                    tamanho.value = 'Min';
                }
                else {
                    especificado.value = "Não há especificado na tabela";
                }
            }
            else if (opcaoSelecionada === 'Látex') {
                especificado.value = "Não há especificado na tabela";
                conforto.value = "Não há especificado na tabela";
            }
            else if (opcaoSelecionada === 'Rabatan') {
                especificado.value = "Não há especificado na tabela";
                conforto.value = "Não há especificado na tabela";
            } else if (opcaoSelecionada === 'Alta resiliência') {
                if (valorDigitado >= '30') {
                    conforto.value = "2, 3";
                    tamanho.value = 'Min';
                }
                especificado.value = "Não há especificado na tabela";
            }
            // Alteração de retornos
            if (opcaoSelecionada === "Aglomerado") {
                reducao.value = 50;
                reducao2.value = 50;
                reducao3.value = 50;
            } else {
                reducao.value = 70;
                reducao2.value = 70;
                reducao3.value = 70;
            }
            if (opcaoSelecionada === 'Visco elástica') {
                condicao.value = "Sim";
            } else {
                condicao.value = "Não";
            }
            // Atualiza o conteúdo da div de resultados

        }



        mostrarResultados(tipo_espuma.value, densidade.value);


    </script>
    @if (TempData.ContainsKey("Mensagem"))
