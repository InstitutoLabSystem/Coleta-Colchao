                        <tr>
                            <
                            <td><input type="text" class="form-control" asp-for="lar_amostra_um_dois" /></td>
                            <td><input type="text" class="form-control" asp-for="lar_amostra_dois_dois" /></td>
                            <td><input type="text" class="form-control" asp-for="lar_amostra_tres_dois" /></td>
